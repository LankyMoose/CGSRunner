{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": ["main"],
  "permissions": [
    "core:default",
    {
      "identifier": "shell:allow-execute",
      "allow": [
        {
          "name": "exec-sh",
          "cmd": "sh",
          "args": true
        }
      ]
    },
    "fs:allow-appdata-write",
    "fs:allow-appdata-read",
    "fs:allow-desktop-read",
    "fs:allow-desktop-write",
    "fs:default"
  ]
}
